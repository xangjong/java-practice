package ch01;



public class Solution {
	 public int[] solution(int n) {
	
	        int[] answer = {n};
	        
	        for(int i=0; i<=answer.length; i++){
	            if( n % 2 != 0){
	            	System.out.println(answer[i]);
	            }
	        }
	        
	        return answer;
	    }
}
